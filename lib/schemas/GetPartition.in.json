{
    "type": "object",
    "properties": {
        "Action": {
            "required": true,
            "type": "string"
        },
        "Version": {
            "required": true,
            "type": "string"
        },
        "requestBody": {
            "properties": {
                "CatalogId": {
                    "description": "The ID of the Data Catalog where the partition in question resides. If none is supplied, the AWS account ID is used by default.",
                    "maxLength": 255,
                    "minLength": 1,
                    "type": "string",
                    "x-pattern": "[\\u0020-\\uD7FF\\uE000-\\uFFFD\\uD800\\uDC00-\\uDBFF\\uDFFF\\t]*"
                },
                "DatabaseName": {
                    "description": "The name of the catalog database where the partition resides.",
                    "maxLength": 255,
                    "minLength": 1,
                    "type": "string",
                    "x-pattern": "[\\u0020-\\uD7FF\\uE000-\\uFFFD\\uD800\\uDC00-\\uDBFF\\uDFFF\\t]*"
                },
                "PartitionValues": {
                    "description": "The values that define the partition.",
                    "items": {
                        "maxLength": 1024,
                        "type": "string"
                    },
                    "type": "array"
                },
                "TableName": {
                    "description": "The name of the partition's table.",
                    "maxLength": 255,
                    "minLength": 1,
                    "type": "string",
                    "x-pattern": "[\\u0020-\\uD7FF\\uE000-\\uFFFD\\uD800\\uDC00-\\uDBFF\\uDFFF\\t]*"
                }
            },
            "required": [
                "DatabaseName",
                "TableName",
                "PartitionValues"
            ],
            "type": "object"
        },
        "X_Amz_Content_Sha256": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Date": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Algorithm": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Credential": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Security_Token": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Signature": {
            "required": false,
            "type": "string"
        },
        "X_Amz_SignedHeaders": {
            "required": false,
            "type": "string"
        }
    }
}